{
  "name": "dataflow-travel",
  "detection": {
    "selectors": [],
    "patterns": []
  },
  "selectors": {
    "title": "h1, title",
    
    "description": ".travel-headline-text-block p, .description",


    "wanted_selectors": [
      ".additional-menu-icon-block",
      ".additional-menu-icon-block",
      ".additional-menu-icon-block",
      ".main-footer-block",
      ".main-search-filter",
      ".main-slider",
      ".mainmenu-block",
      ".megaheader-block",
      ".office-list-block",
      ".product-link-block",
      ".products-all-block",
      ".question-toggle-block",
      ".social-mosaic-block",
      ".travel-booking-management-block",
      ".travel-breadcrumb-block",
      ".travel-contact-form-block",
      ".travel-contact-reservation-form-block",
      ".travel-content-text-area-block",
      ".travel-course-management",
      ".travel-events-list-block",
      ".travel-group-orders-form-block",
      ".travel-headline-text-block",
      ".travel-highlightbox-bg-image-block",
      ".travel-icon-text-block",
      ".travel-image-gallery-block",
      ".travel-image-text-button-list-block",
      ".travel-image-video-collage-block",
      ".travel-info-table",
      ".travel-invoice-offer-block",
      ".travel-main-footer-block",
      ".travel-mainmenu-block",
      ".travel-main-slider",
      ".travel-new-reservation-block",
      ".travel-order-feedback",
      ".travel-product-card-block",
      ".travel-profile-card",
      ".travel-reservation-summary-block",
      ".travel-search-page-block",
      ".travel-social-slider-block",
      ".travel-sub-footer-block",
      ".travel-subpage-hero-img-block",
      ".travel-text-image-video-block",
      ".travel-text-image-video-graphic-list-block",
      ".travel-voucher-activation-page-block",
      ".trustmary-block",
      ".user-image-management",
      ".user-image-slider",
      ".image-text-mosaic-block"
    ],
    
    "hero": {
      "container": ".travel-main-slider .item",
      "title": "h2, .item-title",
      "subtitle": ".h3, p",
      "cta": "a, button"
    },
    
    "experiences": {
      "container": ".travel-highlightbox-bg-image-block .hlbox",
      "title": "h2, h3",
      "description": "p",
      "price": ".price",
      "image": "img",
      "action": "a",
      "link": "a"
    },
    
    "accommodations": {
      "container": ".travel-cabin-list-highlights-block .cabin-item",
      "title": "h3, h4",
      "price": ".price",
      "capacity": "p",
      "image": "img",
      "link": "a"
    },
    
    "tours": {
      "container": ".travel-image-text-button-list-block .list-item",
      "title": "h3",
      "description": ".description, p",
      "price": ".subtitle, p.subtitle",
      "image": "img",
      "link": "a.btn"
    },
 
    "blog_posts": {
      "container": ".blog-list-hl-block .item, article",
      "title": "h3, h4",
      "date": "p, time",
      "excerpt": "p",
      "link": "a"
    },
    
    "footer_info": {
      "container": ".footer-content-column",
      "content": "p",
      "phone": "a[href^='tel:']",
      "email": "a[href^='mailto:']"
    },
    
    
    "logos": {
      "container": "img[alt*='logo'], img[alt*='Logo'], .logo img, [class*='logo'] img, header img",
      "src": "src",
      "alt": "alt"
    },
    
    "colors": {
      "elements": "body, header, footer, nav, main, .header, .footer, .navbar, .hero, h1, h2, h3, a, button, .btn",
      "properties": ["background-color", "color"],
      "maxColors": 10
    },
    
    "unwanted_selectors": [
      ".cookie-block",
      "#cookieDiv",
      "[id='cookieDiv']",
      ".cookie-compact",
      "[class*='cookie-compact']",
      ".row.cookie-compact",
      "[class*='cookie']",
      "[id*='cookie']",
      "[id*='notification']",
      "[class*='notification']",
      "nav",
      ".top-menu",
      ".admin-bar",
      "aside",
      "script",
      "style",
      "noscript",
      "[class*='privacy']",
      "[id*='privacy']",
      "[class*='gdpr']",
      "[id*='gdpr']",
      "[class*='consent']",
      "[id*='consent']",
      "[class*='banner']",
      "[id*='banner']",
      "[class*='popup']",
      "[id*='popup']",
      "[class*='modal']",
      "[id*='modal']",
      "[role='navigation']",
      "[role='banner']",
      "[role='complementary']"
    ]
  },
  "extractors": {
    "custom": ["colors"]
  }
}
