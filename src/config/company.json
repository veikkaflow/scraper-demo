{
  "name": "company",
  "detection": {
    "selectors": [],
    "patterns": []
  },
  "sitemap": {
    "enabled": true,
    "priority": false
  },
  "selectors": {
    "title": "h1, .page-title, .site-title, .hero-title",
    "description": ".description, .content, .entry-content, p",
    "contact": {
      "container": ".contact, .contact-info, [class*='contact']",
      "email": "[href^='mailto:'], .email, [class*='email']",
      "phone": "[href^='tel:'], .phone, [class*='phone']",
      "address": ".address, [class*='address']"
    },
    "services": {
      "container": ".service, [class*='service'], .feature",
      "title": "h2, h3, .service-title",
      "description": "p, .service-description"
    },
    "unwanted_selectors": [
      ".cookie-block",
      "[class*='cookie']",
      "[id*='cookie']",
      "#notification-block",
      "[class*='notification']",
      "nav",
      "header",
      "footer",
      "aside",
      "script",
      "style",
      "noscript"
    ]
  },
  "extractors": {
    "custom": []
  }
}

